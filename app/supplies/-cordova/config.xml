<?xml version='1.0' encoding='utf-8'?>
<widget id="org.js.mol.app.supplies" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Supplies</name>
    <description>
        Company supplies
    </description>
    <author email="nin-jin@ya.ru" href="https://github.com/nin-jin/">
        Jin
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="phonegap-plugin-barcodescanner" spec="4" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="android-minSdkVersion" value="19" />
	<preference name="windows-appx-target" value="uap" />
	<preference name="windows-arch" value="x64" />
	<preference name="SplashScreenDelay" value="0" />
	<plugin name="cordova-plugin-statusbar" spec="^2.2.3" />
	<preference name="StatusBarOverlaysWebView" value="false" />
	<preference name="StatusBarBackgroundColor" value="#000000" />
</widget>
