$mol_string $mol_view
	dom_name \input
	enabled true
	debounce 200
	minimal_height 40
	field *
		^
		disabled <= disabled false
		value <= value_changed?val <=> value?val \
		placeholder <= hint \
		type <= type?val \text
	attr *
		^
		maxlength <= length_max Infinity
	event *
		^
		input?event <=> event_change?event null
		keypress?event <=> event_key_press?event null

$mol_icon_plus $mol_icon path \M19 13h-6v6h-2v-6H5v-2h6V5h2v6h6v2z

$mol_number $mol_view
	precision_view <= precision 1
	precision_change <= precision 1
	value?val NaN
	sub /
		<= Dec $mol_button_minor
			event_click?val <=> event_dec?val null
			enabled <= dec_enabled <= enabled true
			sub / <= dec_icon $mol_icon_minus
		<= String $mol_string
			type \number
			value?val <=> value_string?val \
			hint <= hint \
			enabled <= string_enabled <= enabled true
			debounce <= debounce 200
		<= Inc $mol_button_minor
			event_click?val <=> event_inc?val null
			enabled <= inc_enabled <= enabled true
			sub / <= inc_icon $mol_icon_plus

$mol_app_slides $mol_view
	attr *
		^
		mol_app_slides_role <= role \ 
	contents?val \
	plugins /
		<= Speech_next $mol_speech
			event_catch?val <=> event_next?val null
			patterns <= speech_next /
				\next
				\forward
				\дальше
				\далее
				\удали
				\вперед
				\перевод
		<= Speech_slide $mol_speech
			event_catch?val <=> event_slide?val null
			patterns <= speech_slide /
				\slide number (\d+)
				\(\d+) slide
				\слайд номер (\d+)
				\(\d+) слайд
		<= Speech_prev $mol_speech
			event_catch?val <=> event_prev?val null
			patterns <= speech_prev /
				\back
				\назад
		<= Speech_start $mol_speech
			event_catch?val <=> event_start?val null
			patterns <= speech_start /
				\to beginning
				\first slide
				\начало
				\первый слайд
		<= Speech_end $mol_speech
			event_catch?val <=> event_end?val null
			patterns <= speech_end /
				\to ending
				\last slide
				\конец
				\последний слайд
		<= Speech_about $mol_speech
			event_catch?val <=> event_about?val null
			patterns <= speech_about /
				\slide about (\S+?)
				\search (\S+?)
				\слайд про (\S+?)
				\найти (\S+?)
				\найди (\S+?)
	sub /
		<= Listener $mol_page
			title <= title -
			tools /
				<= Slide_number $mol_view
					sub /
						<= slide?val 0
			body /
				<= Listener_content $mol_text
					uri_base <= uri_base \
					text <= listener_content \
				<= Progress $mol_portion
					portion <= progress 0
		<= Speaker $mol_page
			title \
			tools /
				<= Open_listener $mol_link
					target \_blank
					hint <= open_listener_hint @ \Open slides window
					arg *
						role \listener
						slide null
					sub /
						<= Open_listener_icon $mol_icon_external
				<= Speech_toggle $mol_check_icon
					Icon <= Speech_toggle_icon $mol_icon_microphone
					checked?val <=> speech_enabled?val false
					hint <= speech_toggle_hint @ \Speech contol
				<= Slide_switcher $mol_number
					value?val <=> slide?val 0
			body /
				<= Speaker_content $mol_text
					uri_base <= uri_base \
					text <= speaker_content \
		<= Loader $mol_view
			dom_name \iframe
			attr *
				src <= uri_slides <= uri_slides_default \https://nin-jin.github.io/slides/orp/
			event *
				load?val <=> event_load?val null
