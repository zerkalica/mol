	title @ \Benchmarking results visualization
	sub /
		<= View $mol_bench
			col_sort?val <=> col_sort?val \mid
			result <= result *

$mol_book_demo $mol_demo_large
	title @ \Adaprive layout for various sizes of screen
	sub /
		<= View $mol_book
			Placeholder <= Placeholder $mol_book_placeholder
				minimal_width 200
				sub / \ Placeholder
			pages /
				<= Addon $mol_view
					minimal_width 250
					sub / \ Addon
				<= Main $mol_view
					minimal_width 400
					sub / \ Main

$mol_button_demo $mol_demo_small
	title @ \All types of buttons in every states
	sub /
		<= Major_enabled $mol_button_major
			title <= major_label @ \Click me!
		-
		<= Major_disabled $mol_button_major
			title <= major_label @ \Click me!
			enabled false
		-
		<= Minor_enabled $mol_button_minor
			title <= minor_label @ \Or click me..
		-
		<= Minor_disabled $mol_button_minor
			title <= minor_label @ \Or click me..
			enabled false
		-
		<= Danger_enabled $mol_button_danger
			title <= danger_label @ \Be attentive!
		-
		<= Danger_disabled $mol_button_danger
			title <= danger_label @ \Be attentive!
			enabled false

$mol_calendar $mol_list
	sub /
		<= Title $mol_view
			minimal_height 24
			sub /
				<= title \
		<= Weekdays $mol_view
			sub /
				<= weekdays /
		<= Weeks $mol_list
			rows /
				<= weeks /
	-
	Weekday!index $mol_calendar_day
		holiday <= weekend!index false
		sub /
			<= weekday!index \
	-
	Week!row $mol_view
		sub / 
			<= week_days!row /
	-
	Day!day $mol_calendar_day
		ghost <= day_ghost!day false
		holiday <= day_holiday!day false
		selected <= day_selected!day false
		sub <= day_content!day /
			<= day_text!day \
	-
	month_string \
	month_moment $mol_time_moment

